@using Shop.Models
@model List<Shop.db.Entities.StaticSpecification>




      
@if (Model != null && Model.Any())
{
    <table class="table table-bordered table-condensed">
        <tr>
            <th>ид</th>
            <th>Название</th>

            @*<th></th>*@
        </tr>
        @foreach (var specs in Model)
        {
            <tr>
                <td>@specs.id</td>
                <td>@specs.name</td>

                @*<td></td>*@
            </tr>
        }

    </table>


}
else
{
        <p>нет данных</p>
 }
